<div class="container devise-form">
  <div class="col-md-5">
    <div class="card">
      <div class="card-header">
        Schedule Game
      </div>
      <div class="card-body">
        <%= form_for @game do |f| %>
          <%= f.hidden_field :season_id, value: @game.season_id %>
          <%= render partial: 'game_form', locals: { button_title: 'Schedule Game', f: f } %>
        <% end %>
      </div>
    </div>
  </div>
</div>
